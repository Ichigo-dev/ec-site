var messages = <%== @address.errors.full_messages %>
<% if @id %>
  var id = <%== @id %>
  $formError = $(`#address-error-${id}`);
<% else %>
  $formError = $('#address-error-new')
<% end %>
$formError.html("");
for (message of messages){
  $formError.append(`<p>${message}</p>`);
}
$formError.show();
